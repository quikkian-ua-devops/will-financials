<databaseChangeLog xmlns='http://www.liquibase.org/xml/ns/dbchangelog'
	xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xsi:schemaLocation='http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd'>

    <changeSet author="KFS601" id="KFSTP-2203_ADD_LOGOUT_PAGE_URL_PARM">
        <insert tableName="KRCR_PARM_T">
            <column name="NMSPC_CD" value="KR-NS" />
            <column name="CMPNT_CD" value="All" />
            <column name="PARM_NM" value="LOGOFF_REDIRECT_URL" />
            <column name="OBJ_ID" value="KR-NS-PARM-001" />
            <column name="VER_NBR" value="1" />
            <column name="PARM_TYP_CD" value="CONFG" />
            <column name="VAL" value="signout.html" />
            <column name="PARM_DESC_TXT"
                    value="Set this value to the URL to redirect to when a user signs out of the application." />
            <column name="EVAL_OPRTR_CD" value="A" />
            <column name="APPL_ID" value="KFS" />
        </insert>
    </changeSet>
    <changeSet author="KFS601" id="KFSCD-6_MANY_TO_MANY_TRANSFERS">
        <comment>KFSCD-6 Block a Many to Many transfer within the TF and YETF documents</comment>
        <insert tableName="KRCR_PARM_T">
            <column name="NMSPC_CD" value="KFS-FP"/>
            <column name="CMPNT_CD" value="Document"/>
            <column name="PARM_NM" value="ALLOW_MANY_TO_MANY_TRANSFERS_IND"/>
            <column name="OBJ_ID" value="KFSCD-6-1"/>
            <column name="PARM_TYP_CD" value="CONFG"/>
            <column name="VAL" value="Y"/>
            <column name="PARM_DESC_TXT" value="Specifies whether or not many-to-many transfers are allowed on the Transfer of Funds and Year End Transfer of Funds documents. Set the parameter to Y to allow many to many transfers."/>
            <column name="EVAL_OPRTR_CD" value="A"/>
            <column name="APPL_ID" value="KFS"/>
        </insert>
    </changeSet>



</databaseChangeLog>
